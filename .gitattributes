public/js/**/*.js linguist-generated=true
public/js/**/*.js.map linguist-generated=true

# Backend compiled output
functions/lib/**/*.js linguist-generated=true
functions/lib/**/*.js.map linguist-generated=true
functions/lib/**/*.d.ts linguist-generated=true
